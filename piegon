class Pigeon:
    def __init__(self, age, weight):
        self.age = age
        self.weight = weight

    def fly(self):
        print("I am Flying now...")

    def land(self):
        print("I am landed!!")

class Eagle(Pigeon):
    def __init__(self, age, weight, attackSpeed):
        self.age = age
        self.weight = weight
        self.attackSpeed = attackSpeed

    def attack(self):
        print("Attacking with the speed of " + str(self.attackSpeed))

e1 = Eagle(12, 30, 46)
e1.fly()
e1.attack()
